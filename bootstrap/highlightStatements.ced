defAtom(topLevel::marker).
check(Path, Statement::statement, VNs, marker(Path, topLevel)):-statementPath(Path).
markerVis(topLevel, Vis, color(Vis, cpi:rgb(0, 0, 128))).
defAtom(notTopLevel::marker).
check(cpi:path(Res, Path), TTerm, VNs, marker(cpi:path(Res, Path), notTopLevel)):-append(PathPrime, [_], Path, number), statementPath(cpi:path(Res, PathPrime)).
markerVis(notTopLevel, Vis, color(Vis, cpi:rgb(0, 0, 0))).
